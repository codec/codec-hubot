export HUBOT_<%= adapter.upcase -%>_USERNAME=<%= username %>
export HUBOT_<%= adapter.upcase -%>_PASSWORD=<%= password %>
export HUBOT_<%= adapter.upcase -%>_ROOMS=<%= rooms.join(',') %>

<% if options %>
    <% options.sort.each do |k,v| -%>
    export HUBOT_<%= k.upcase -%>="<%= v %>"
    <%- end %>
<%- end %>